#include "main.h"
#include <string.h>

/**
* _strncat - Joins two string but you can choose the number of characters to include
* @dest: string
* @src: string
* @n: Number of characters to include
* Return: return dest
*/

char *_strncat(char *dest, char *src, int n)
{
	strncat(dest, src, n);

	return (dest);

}
